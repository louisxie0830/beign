{
  "name": "being-document-signing-backend",
  "version": "4.5.2",
  "private": true,
  "scripts": {
    "prod": "pm2-runtime start ecosystem.config.js",
    "start": "node src/app.js",
    "dev": "nodemon",
    "lint": "eslint src test unit-test",
    "test": "mocha unit-test/**/*.spec.js",
    "test-hapi": "lab -a @hapi/code -l -c -v",
    "test-tap": "lab -a @hapi/code -l -r tap -o tests.tap",
    "test-cov-html": "lab -a @hapi/code -l -r html -o coverage.html"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.js",
      "**/*.spec.js",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run start"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@hapi/address": "^3.2.2",
    "@hapi/boom": "^8.0.1",
    "@hapi/good": "^8.2.4",
    "@hapi/good-console": "^8.1.2",
    "@hapi/hapi": "^18.4.0",
    "@hapi/hoek": "^8.5.0",
    "@hapi/inert": "^5.2.2",
    "@hapi/joi": "^16.1.8",
    "@hapi/joi-date": "^2.0.1",
    "@hapi/vision": "^5.5.4",
    "@promster/hapi": "^4.0.0",
    "accept-language-parser": "^1.5.0",
    "ali-oss": "^6.3.1",
    "alipay-sdk": "^3.0.8",
    "aws-sdk": "^2.586.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "bignumber.js": "^9.0.0",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^8.2.0",
    "handlebars": "^4.5.3",
    "handlebars-helpers": "^0.10.0",
    "hapi-mysql2": "^2.4.2",
    "hapi-redis2": "^2.1.3",
    "hapi-sequelizejs": "^3.1.0",
    "hapi-swagger": "^11.1.0",
    "hummus-recipe": "^1.8.24",
    "iconv-lite": "^0.5.0",
    "image-size": "^0.8.3",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql2": "^2.0.2",
    "node-crc": "^1.2.2",
    "node-exiftool": "^2.3.0",
    "nodemailer": "^6.4.1",
    "pm2": "^4.2.0",
    "prom-client": "^11.5.3",
    "puppeteer": "^2.0.0",
    "randomstring": "^1.1.5",
    "redis-leader": "^0.1.0",
    "redlock": "^4.1.0",
    "sequelize": "^5.21.2",
    "text2png": "^2.3.0",
    "uuid": "^3.3.3",
    "web3": "1.0.0-beta.37",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "@hapi/code": "^7.0.0",
    "@hapi/lab": "^21.0.0",
    "ajv": "^6.10.2",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "editorconfig": "^0.15.3",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "form-data": "^3.0.0",
